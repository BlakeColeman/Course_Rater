CREATE TABLE roles(role_id int, rolename varchar(255),PRIMARY KEY(role_id);
CREATE TABLE users(lname varchar(255),dname varchar(255),pword varchar(255),role_id int,PRIMARY KEY (lname),FOREIGN KEY (role_id) REFERENCES roles(role_id) );
CREATE TABLE courses(cid int,cname varchar(255),PRIMARY KEY(cid));
CREATE TABLE teachers(tid int, tname varchar(255),lname varchar(255),PRIMARY KEY(tid));
CREATE TABLE teaches(cid int,tid int, tdesc varchar(255),syllabus varchar(255)PRIMARY KEY(cid,tid), FOREIGN KEY(cid) REFERENCES courses(cid),FOREIGN KEY(tid) REFERENCES teachers(tid));
CREATE TABLE reviews(review_id int,cid int, rdesc varchar(255),crating int, rreview int, flags int,PRIMARY KEY(review_id),FOREIGN KEY(cid) REFERENCES courses(cid));
